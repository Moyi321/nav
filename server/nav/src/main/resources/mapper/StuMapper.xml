<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.qdu.nav.mapper.StuMapper">

  <select id="check" parameterType="java.lang.String" resultType="com.qdu.nav.entity.PO.StuInfo">
    select
      *
    from stu_info
    where
          name = #{name}
      and
          ss_number = #{ssNumber}
    limit 1
  </select>

  <select id="search" parameterType="java.lang.String" resultType="com.qdu.nav.entity.PO.StuInfo">
    select
      *
    from stu_info
    where
      name like concat('%',#{name},'%')
  </select>

</mapper>
